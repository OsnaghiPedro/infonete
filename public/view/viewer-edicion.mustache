<script src="/infonete/public/js/vendors/misc.js"></script>
<script src="/infonete/public/js/vendors/off-canvas.js"></script>
-->
<script src="/infonete/public/js/vendors/dashboard.js"></script>


<script
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBeluO_jCvIS_iT6Y3Thw8A6YJW5gyzh0M&callback=init_Preview&libraries=places&v=weekly"
        defer>

</script>

<script src="/infonete/public/js/vendors/viewmaps.js"></script>

<script>init_Preview();</script>

